console_impl=qemu
qemu_machine="virt"
qemu_binary="qemu-system-riscv64"
qemu_extra_args="-m 1G -nographic -netdev user,tftp=${UBOOT_TRAVIS_BUILD_DIR} -device virtio-net-device,netdev=user"
qemu_kernel_args="-kernel ${U_BOOT_BUILD_DIR}/u-boot"
reset_impl=none
flash_impl=none
